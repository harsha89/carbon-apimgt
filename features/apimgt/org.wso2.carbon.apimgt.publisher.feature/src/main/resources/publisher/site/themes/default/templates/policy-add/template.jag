    <legend>General Details</legend>
     <div class="control-group">
        <label class="control-label" for="policy-name">Policy Name:<span class="requiredAstrix">*</span></label>
        <div class="controls">
            <input type="text" class="input-xlarge required validRegistryName validateForwardSlashAtEnd validInput apiNameExists noSpace" id="policy-name" name="policy-nam" autofocus="autofocus" value="">
            <a class="icon-question-sign help_popup" data-trigger="hover" help_data="name_help" data-original-title="" title=""></a>
            <p id="name_help" class="hide">Name of the throttle policy.</p>
        </div>
    </div>

    <div class="control-group">
        <label class="control-label" for="policy-description">Policy Description:<span class="requiredAstrix">*</span></label>
        <div class="controls">
            <input type="text" class="input-xlarge required validRegistryName validateForwardSlashAtEnd validInput apiNameExists noSpace" id="policy-description" name="policy-description" autofocus="autofocus" value="">
